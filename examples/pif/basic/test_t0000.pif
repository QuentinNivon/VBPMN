<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Process xmlns="http://www.example.org/PIF" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <name>basic1</name>
    <documentation>test process basic 1</documentation>
    <peers id="peer1"/>
    <peers id="peer2"/>
    <messages id="message1"/>
    <messages id="message2"/>
    <behaviour>
        <nodes xsi:type="InitialEvent" id="start">
            <outgoingFlows>sf1</outgoingFlows>
        </nodes>
        <nodes xsi:type="EndEvent" id="stop">
            <incomingFlows>sf3</incomingFlows>
        </nodes>
        <nodes xsi:type="Task" id="task">
            <incomingFlows>sf1</incomingFlows>
            <outgoingFlows>sf2</outgoingFlows>
        </nodes>
        <nodes xsi:type="MessageReception" message="message1" id="x1">
            <incomingFlows>sf2</incomingFlows>
            <outgoingFlows>sf3</outgoingFlows>
        </nodes>
        <sequenceFlows id="sf1" source="start" target="task"/>
        <sequenceFlows id="sf2" source="task" target="x1"/>
        <sequenceFlows id="sf3" source="x1" target="stop"/>
        <initialNode>start</initialNode>
    </behaviour>
</Process>
